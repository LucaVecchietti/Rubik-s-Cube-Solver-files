<ColorPopup@Popup>:
    id: color_popup
    size_hint: None, None
    size: "400dp", "150dp"
    BoxLayout:
        orientation: 'horizontal'
        padding: "10dp", "20dp", "10dp", "20dp"
        spacing: "15dp"
        Button:
            background_color: 255, 255, 255, 1
            size: "45dp", "45dp"
            on_press: root.dismiss()
            on_press: root.white_button()
            on_release: root.change_color()
        Button:
            background_color: 0, 0, 255, 1
            size: "45dp", "45dp"
            on_press: root.dismiss()
            on_press: root.blue_button()
            on_release: root.change_color()
        Button:
            background_color: 255, 0, 0, 1
            size: "45dp", "45dp"
            on_press: root.dismiss()
            on_press: root.red_button()
            on_release: root.change_color()
        Button:
            background_color: 0, 255, 0, 1
            size: "45dp", "45dp"
            on_press: root.dismiss()
            on_press: root.green_button()
            on_release: root.change_color()
        Button:
            background_color: 255, .7, 0, 1
            size: "45dp", "45dp"
            on_press: root.dismiss()
            on_press: root.orange_button()
            on_release: root.change_color()
        Button:
            background_color: 255, 255, .2, 1
            size: "45dp", "45dp"
            on_press: root.dismiss()
            on_press: root.yellow_button()
            on_release: root.change_color()


